    <!-- ABOUT -->
    <section class="pt-5 pt-md-7">
      <div class="container">
        <div class="row align-items-center">
          <div class="col-12 col-md-6">

            <!-- Heading -->
            <h2 class="fw-bold">
              De l'experience dans diverse domaine informatique
            </h2>

            <!-- Text -->
            <p class="fs-lg text-muted mb-5">
              Enquetes, ecoutes, audits, ne sont qu'une partit du spectre d'activité 
            </p>

            <!-- Button -->
            <a href="/contact" class="btn btn-primary-desat mb-6 mb-md-0 lift">
              Contactez nous! <i class="fe fe-arrow-right ms-3"></i>
            </a>

          </div>
          <div class="col-12 col-md-6">

