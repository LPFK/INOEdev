                </div>

                <!-- Macbook -->
                <div class="device device-macbook">
                  <img src="/img/screenshots/desktop/dashki t-alt.jpg" class="device-screen" alt="...">
                  <img src="/img/devices/macbook.svg" class="img-fluid" alt="...">
                </div>

              </div>
            </div>

          </div>
        </div> <!-- / .row -->
      </div> <!-- / .container -->
    </section>

