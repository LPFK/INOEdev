        <div class="row align-items-center text-center text-md-start">
          <div class="col-12 col-md-6">

            <!-- Image -->
            <img src="/img/illustrations/illustration-4.png" alt="Image d'un homme interagissant avec un smartphone" class="img-fluid mw-130 float-end mb-6 mb-md-0" data-aos="fade-right">

          </div>
          <div class="col-12 col-md-6">

            <!-- Heading -->
            <h1 class="display-1 fw-bold">
